<Window x:Class="ProfessionalProfile.view.TakeTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProfessionalProfile.view"
        mc:Ignorable="d"
        Title="TakeTestWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="TestNameLabel" Content="Label" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.39,-0.129"/>
        <ScrollViewer x:Name="QuestionsArea" Margin="100,50,100,80">
            <StackPanel x:Name="QuestionsList" Margin="10,10,10,10"/>
        </ScrollViewer>
        <Button x:Name="SubmitTestButton" Content="Submit" HorizontalAlignment="Center" Margin="0,381,0,0" VerticalAlignment="Top" Click="SubmitTestButton_Click"/>

    </Grid>
</Window>
