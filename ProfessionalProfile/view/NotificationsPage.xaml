<Window x:Class="ProfessionalProfile.view.NotificationsPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProfessionalProfile.view"
        mc:Ignorable="d"
        Title="NotificationsPage" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="161,0,162,0">
            <Label x:Name="nameLabel" Content="Label" FontSize="14" FontWeight="Bold"/>
            <ScrollViewer Height="381">
                <ListBox x:Name="notificationsList" />
            </ScrollViewer>
        </StackPanel>

    </Grid>
</Window>
